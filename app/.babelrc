{
    "presets": [
        ["env",  { "modules": false }],
        "react"
    ],
    "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
    ],
    "env": {
        "test": {
            "presets": [["env"], "react"],"plugins": [
                "transform-decorators-legacy",
                "transform-class-properties",
                "transform-object-rest-spread",
                ["inline-react-svg", {
                    "svgo": {
                        "plugins": [
                            { "cleanupIDs": false },
                            { "removeTitle": true }
                        ]
                    }
                }]
            ] }
    }
}
